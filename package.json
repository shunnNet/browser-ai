    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "happy-dom": "^13.8.2",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "typescript": "^5.0.2",
